{
    "name": "codebyray/laravel-review-rateable",
    "description": "Review & Rating system for Laravel 10, 11 & 12",
    "keywords": ["rating", "Ratable", "laravel", "Review-Rateable", "reviewable"],
    "license": "MIT",
    "authors": [
        {
            "name": "CodebyRay",
            "email": "dev@codebyray.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/codebyray/laravel-review-rateable"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/config": "^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "pestphp/pest": "^1.20 || ^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "Codebyray\\ReviewRateable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codebyray\\ReviewRateable\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codebyray\\ReviewRateable\\ReviewRateableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
